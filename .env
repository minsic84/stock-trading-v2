# Application Environment
ENVIRONMENT=development
DEBUG=True
LOG_LEVEL=INFO

# Database Configuration (SQLite)
DB_TYPE=sqlite
SQLITE_DB_PATH=./data/stock_data.db

# Kiwoom API Settings (나중에 입력)
KIWOOM_USER_ID=your_kiwoom_id
KIWOOM_PASSWORD=your_kiwoom_password
KIWOOM_CERT_PASSWORD=your_cert_password

# API Settings
API_REQUEST_DELAY_MS=3600
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY_SECONDS=3.6